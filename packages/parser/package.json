{
  "name": "parser",
  "version": "0.0.2",
  "description": "The parsing library that underlies Gramble",
  "keywords": [
    "parsing",
    "automata",
    "morphology"
  ],
  "author": "Patrick Littell <Patrick.Littell@nrc-cnrc.gc.ca>",
  "homepage": "https://github.com/littell/gramble_ts#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littell/gramble_ts.git"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register tests/*.ts",
    "testBasics": "mocha -r ts-node/register tests/testBasics.ts",
    "testDot": "mocha -r ts-node/register tests/testDot.ts",
    "testEmbed": "mocha -r ts-node/register tests/testEmbed.ts",
    "testFilter": "mocha -r ts-node/register tests/testFilter.ts",
    "testIntersection": "mocha -r ts-node/register tests/testIntersection.ts",
    "testJoins": "mocha -r ts-node/register tests/testJoins.ts",
    "testMatch": "mocha -r ts-node/register tests/testMatch.ts",
    "testNegation": "mocha -r ts-node/register tests/testNegation.ts",
    "testProj": "mocha -r ts-node/register tests/testProjection.ts",
    "testRename":  "mocha -r ts-node/register tests/testRename.ts",
    "testRepeat":  "mocha -r ts-node/register tests/testRepeat.ts",
    "testRepeatDot":  "mocha -r ts-node/register tests/testRepeatDot.ts",
    "testSheets": "mocha -r ts-node/register tests/testSheetParser.ts",
    "testHeaders": "mocha -r ts-node/register tests/testHeaders.ts",
    "testProperties": "mocha -r ts-node/register tests/property/testProperties.ts",    
    "testAst": "mocha -r ts-node/register tests/asts/*.ts",
    "testHideAst": "mocha -r ts-node/register tests/asts/testHideAst.ts",
    "testRenameAst": "mocha -r ts-node/register tests/asts/testRenameAst.ts",
    "testBasicsAst": "mocha -r ts-node/register tests/asts/testBasicsAst.ts",
    "testFilterAst": "mocha -r ts-node/register tests/asts/testFilterAst.ts",
    "testNegationAst": "mocha -r ts-node/register tests/asts/testNegationAst.ts",
    "testJoinsAst": "mocha -r ts-node/register tests/asts/testJoinsAst.ts",
    "testRepeatAst": "mocha -r ts-node/register tests/asts/testRepeatAst.ts",
    "testDotAst": "mocha -r ts-node/register tests/asts/testDotAst.ts",
    "testMatchAst": "mocha -r ts-node/register tests/asts/testMatchAst.ts",
    "testEmbedAst": "mocha -r ts-node/register tests/asts/testEmbedAst.ts",
    "testRecursionAst": "mocha -r ts-node/register tests/asts/testRecursionAst.ts",
    "testSheetsAst": "mocha -r ts-node/register tests/asts/testSheetsAst.ts"
  },
  "bugs": {
    "url": "https://github.com/littell/gramble_ts/issues"
  },
  "dependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "bitset": "^5.1.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "mocha": "^8.3.2",
    "mocha-performance": "^0.1.1",
    "tsify": "^5.0.2",
    "typescript": "^4.1.2"
  }
}
