{
  "name": "@gramble/parser",
  "version": "0.0.2",
  "description": "The parsing library that underlies Gramble",
  "keywords": [
    "parsing",
    "automata",
    "morphology"
  ],
  "author": "Patrick Littell <Patrick.Littell@nrc-cnrc.gc.ca>",
  "homepage": "https://github.com/littell/gramble_ts#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littell/gramble_ts.git"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register tests/*.ts tests/sheets/*/*.ts",
    "testGrammars": "mocha -r ts-node/register tests/*.ts",
    "testSheets": "mocha -r ts-node/register tests/sheets/*/*.ts",
    "testHeaders": "mocha -r ts-node/register tests/testHeaders.ts",
    "testProperties": "mocha -r ts-node/register tests/property/testProperties.ts",
    "testHide": "mocha -r ts-node/register tests/testHide.ts",
    "testRename": "mocha -r ts-node/register tests/testRename.ts",
    "testBasics": "mocha -r ts-node/register tests/testBasics.ts",
    "testFilter": "mocha -r ts-node/register tests/testFilter.ts",
    "testNegation": "mocha -r ts-node/register tests/testNegation.ts",
    "testJoins": "mocha -r ts-node/register tests/testJoins.ts",
    "testRepeat": "mocha -r ts-node/register tests/testRepeat.ts",
    "testIntersection": "mocha -r ts-node/register tests/testIntersection.ts",
    "testDot": "mocha -r ts-node/register tests/testDot.ts",
    "testMatch": "mocha -r ts-node/register tests/testMatch.ts",
    "testMatchRename": "mocha -r ts-node/register tests/testMatchRename.ts",
    "testMatchFrom": "mocha -r ts-node/register tests/testMatchFrom.ts",
    "testMatchDanger": "mocha -r ts-node/register tests/testMatchDanger.ts",
    "testNotMatch": "mocha -r ts-node/register tests/testNotMatch.ts",
    "testEmbed": "mocha -r ts-node/register tests/testEmbed.ts",
    "testRecursion": "mocha -r ts-node/register tests/testRecursion.ts",
    "testRegex": "mocha -r ts-node/register tests/testRegex.ts",
    "testReplace": "mocha -r ts-node/register tests/testReplace.ts",
    "testJoinReplace": "mocha -r ts-node/register tests/testJoinReplace.ts",
    "testNotReplace": "mocha -r ts-node/register tests/testNotReplace.ts",
    "testTokenize": "mocha -r ts-node/register tests/testTokenize.ts",
    "testSheetsBasics": "mocha -r ts-node/register tests/sheets/basics/*.ts",
    "testSyntaxErrors": "mocha -r ts-node/register tests/sheets/syntaxErrors/*.ts",
    "testSheetsFilters": "mocha -r ts-node/register tests/sheets/filters/*.ts",
    "testMultiSheet": "mocha -r ts-node/register tests/sheets/multi/*.ts",
    "testUnitTests": "mocha -r ts-node/register tests/sheets/unitTests/*.ts",
    "testSheetsReplace": "mocha -r ts-node/register tests/sheets/replace/*.ts",
    "testSheetsCasing": "mocha -r ts-node/register tests/sheets/casing/*.ts",
    "testSheetsBare": "mocha -r ts-node/register tests/sheets/bare/*.ts",
    "testSheetsOps": "mocha -r ts-node/register tests/sheets/ops/*.ts",
    "testSheetsHide": "mocha -r ts-node/register tests/sheets/hide/*.ts",
    "testSheetsRename": "mocha -r ts-node/register tests/sheets/rename/*.ts",
    "testSheetsRegex": "mocha -r ts-node/register tests/sheets/regex/*.ts",
    "testSheetsNamespace": "mocha -r ts-node/register tests/sheets/namespace/*.ts"
  },
  "bugs": {
    "url": "https://github.com/littell/gramble_ts/issues"
  },
  "dependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "bitset": "^5.1.1",
    "unicode-properties": "^1.3.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "mocha": "^8.3.2",
    "mocha-performance": "^0.1.1",
    "tsify": "^5.0.2",
    "typescript": "^4.1.2"
  }
}
